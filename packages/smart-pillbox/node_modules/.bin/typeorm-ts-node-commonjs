#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/typeorm@0.3.22_mysql2@3.14.1_redis@4.7.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+cor_8579b80c12d1820f253cf3dd7d5a6773/node_modules/typeorm/node_modules:/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/typeorm@0.3.22_mysql2@3.14.1_redis@4.7.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+cor_8579b80c12d1820f253cf3dd7d5a6773/node_modules:/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/typeorm@0.3.22_mysql2@3.14.1_redis@4.7.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+cor_8579b80c12d1820f253cf3dd7d5a6773/node_modules/typeorm/node_modules:/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/typeorm@0.3.22_mysql2@3.14.1_redis@4.7.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+cor_8579b80c12d1820f253cf3dd7d5a6773/node_modules:/Users/miyumao/WebProject/time_letters_server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
